# File: ../../campaigns/atl01.wmf/scripting/texts.lua, line: 355
# File: ../../campaigns/atl01.wmf/scripting/texts.lua, line: 356
# File: ../../campaigns/atl01.wmf/scripting/texts.lua, line: 376
