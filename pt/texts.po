msgid "E        Toggles event menu"
msgstr "E        Liga/desliga Menu de eventos"

msgid "F        Toggles fullscreen"
msgstr "F        Alterna o Ecrã completo"

msgid "H        Toggles main menu"
msgstr "H        Alterna o Menu Principal"

msgid "M        Toggles minimap"
msgstr "M        Liga e desliga o mini-mapa"

msgid "P        Toggles player menu"
msgstr "P        Liga/desliga o Menu do Jogador"

msgid "T        Toggles tools menu"
msgstr "T        Alterna o Menu de Ferramentas"

msgid "I        Activates information tool"
msgstr "I        Activa a ferramenta informativa"

msgid "CTRL+L   Load Map"
msgstr "CTRL+L   Carregar o mapa"

msgid "CTRL+S   Save Map"
msgstr "CTRL+S   Salvar o mapa"

msgid "1-0      Changes tool size"
msgstr ""

msgid "SHIFT    (Hold) Selects first alternative tool while pressed"
msgstr ""
"SHIFT    (manter) Selecciona primeira ferramenta alternativa enquanto "
